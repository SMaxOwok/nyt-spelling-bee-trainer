{
  "name": "NYT Spelling Bee Trainer",
  "version": "1.3",
  "description": "Display answers sorted by length in game window.",
  "manifest_version": 2,
  "author": "Max Ono",

  "permissions": ["storage"],

  "content_scripts": [
    {
      "matches": ["*://www.nytimes.com/puzzles/spelling-bee"],
      "all_frames": true,
      "js": ["index.js"],
      "run_at": "document_end"
    }
  ]
}